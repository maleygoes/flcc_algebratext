				<paragraphs xml:id="simplifying-terms-1" class="unnumbered">
					<title>Simplifying Terms</title>

					<p>
						Rewrite each of the following terms in simplified form.
					</p>

<!-- div attr= class="tasks"-->
					<p>
						(2)
					</p>

<!-- div attr= class="exercise"-->
					<p>
						<m>2x(3x^2)</m>
					</p><!--</div attr= class="exercise">-->

<!-- div attr= class="solution"-->
					<p>
						<m>6x^3</m>
					</p><!--</div attr= class="solution">-->

<!-- div attr= class="exercise"-->
					<p>
						<m>4(2y^2)(7y^2)</m>
					</p><!--</div attr= class="exercise">-->

<!-- div attr= class="exercise"-->
					<p>
						<m>(3x^2)(5x)(2x^4)</m>
					</p><!--</div attr= class="exercise">-->

<!-- div attr= class="solution"-->
					<p>
						<m>30x^7</m>
					</p><!--</div attr= class="solution">-->

<!-- div attr= class="exercise"-->
					<p>
						<m>(7y)(2y^5)(3y^2)</m>
					</p><!--</div attr= class="exercise">-->

<!-- div attr= class="exercise"-->
					<p>
						<m>(3z^3)(4z^4)</m>
					</p><!--</div attr= class="exercise">-->

<!-- div attr= class="solution"-->
					<p>
						<m>12z^7</m>
					</p><!--</div attr= class="solution">-->

<!-- div attr= class="exercise"-->
					<p>
						<m>(2p^2)(3p^3)(4p^4)</m>
					</p><!--</div attr= class="exercise">-->

<!-- div attr= class="exercise"-->
					<p>
						<m>(x^2y^3)(x^5)(3x^4)</m>
					</p><!--</div attr= class="exercise">-->

